fn main() {
    let i = 0;
    let list = [];

    while i < 10 {
        list.push(i);
        i = i + 1;
    };

    assert(list.len() == 10);

    println(list);

    {
        let i = 0;

        while i < list.len() {
            list[i] = list[i] * 2;
            i = i + 1;
        };
    };

    println(list);
}

main();
